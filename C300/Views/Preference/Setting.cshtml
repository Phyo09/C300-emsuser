<h2 style="color:orange ; font-weight: bold;">Preference</h2>


<form id="myform" asp-controller="Preference" asp-action="Setting" class="form-horizontal col-sm-offset-1" role="form">
    <span style="color:red">@TempData["Msg"]</span><br />
    <div class="form-group row" style="">
        @*Temperature*@
        <h3>Temperature</h3>
        <label class="control-label col-sm-1" asp-for="LowestTemp">Lowest: ('C)</label>
        <div class="col-sm-1">
            <input id="txtLowTemp" asp-for="LowestTemp" class="form-control" readonly="readonly" value="@Model.LowestTemp"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" asp-for="HighestTemp">Highest: ('C)</label>
        <div class="col-sm-1">
            <input id="txtHighTemp" asp-for="HighestTemp" class="form-control" readonly="readonly" value="@Model.HighestTemp"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" asp-for="TempUnit" style="width:6%">Unit:</label>
        <div class="col-sm-2">
            <select class="form-control" asp-for="TempUnit" disabled style="font-family: Verdana;font-size: small;">
                <option value="1">Celcius ('C)</option>
                <option value="2">Kelvin (K)</option>
                <option value="3">Fahrenheit ('F)</option>
            </select>
        </div>
        <label class="col-sm-2 control-label" style="width:11%" asp-for="TempNoti">Notification:</label>
        <div class="col-sm-1" style="width:10%">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="TempNoti">
                <option value="1">On</option>
                <option value="0">Off</option>
            </select>
        </div>
        <div class="col-sm-1">
            @*<a class="col-sm-1 control-label" id="tempEdit">Edit</a>*@
            <button type="button" class="btn-info edit" data-toggle="modal" data-target="#editTemp">Edit</button>
            <div class="col-sm-1 control-label">
                <input id="tempSave" class="btn btn-primary" type="submit" value="Save">
                <input id="tempReset" class="btn btn-primary" type="reset" value="Reset">
            </div>
        </div>
    </div>

    @*Humidity*@
    <div class="form-group row">

        <h3>Humidity</h3>
        <label class="control-label col-sm-1" asp-for="LowestHumidity">Lowest: (%)</label>
        <div class="col-sm-1">
            <input id="txtLowHum" asp-for="LowestHumidity" class="form-control" readonly="readonly" value="@Model.LowestHumidity"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" asp-for="LowestHumidity">Highest: (%)</label>
        <div class="col-sm-1">
            <input id="txtHighHum" asp-for="HighestHumidity" class="form-control" readonly="readonly" value="@Model.HighestHumidity"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" style="width:6%" asp-for="HumUnit">Unit:</label>
        <div class="col-sm-2">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="HumUnit">
                <option value="1">Percentage(%)</option>
            </select>
        </div>

        <label class="col-sm-2 control-label" style="width:11%" asp-for="HumNoti">Notification:</label>
        <div class="col-sm-1" style="width:10%">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="HumNoti">
                <option value="1">On</option>
                <option value="0">Off</option>
            </select>
        </div>
        <div class="col-sm-1">
            @*<a id="humEdit">Edit</a>*@
            <button type="button" class="btn-info edit" data-toggle="modal" data-target="#editHum">Edit</button>
            <input id="humSave" class="btn btn-primary" type="submit" value="Save">
            <input id="humReset" class="btn btn-primary" type="reset" value="Reset">
        </div>
    </div>

    @*Light*@
    <div class="form-group row">
        <h3>Light</h3>
        <label class="control-label col-sm-1" asp-for="LowestLight">Lowest: (lx)</label>
        <div class="col-sm-1">
            <input id="txtLowLight" asp-for="LowestLight" class="form-control" readonly="readonly" value="@Model.LowestLight"
                   style="font-family: Verdana;font-size: small;" />
        </div>
        <label class="control-label col-sm-1" asp-for="LowestLight">Highest: (lx)</label>
        <div class="col-sm-1">
            <input id="txtHighLight" asp-for="HighestLight" class="form-control" readonly="readonly" value="@Model.HighestLight"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" style="width:6%" asp-for="LightUnit">Unit:</label>
        <div class="col-sm-2">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="LightUnit">
                <option value="1">lx</option>
            </select>
        </div>
        <label class="col-sm-2 control-label" style="width:11%" asp-for="LightNoti">Notification:</label>
        <div class="col-sm-1" style="width:10%">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="LightNoti">
                <option value="1">On</option>
                <option value="0">Off</option>
            </select>
        </div>
        <div class="col-sm-1">
            @*<a id="lightEdit">Edit</a>*@
            <button type="button" class="btn-info edit" data-toggle="modal" data-target="#editLight">Edit</button>
            <input id="lightSave" class="btn btn-primary" type="submit" value="Save">
            <input id="lightReset" class="btn btn-primary" type="reset" value="Reset">
        </div>
    </div>

    @*Weight*@
    <div class="form-group row">
        <h3>Force</h3>
        <label class="control-label col-sm-1" asp-for="LowestWeight">Lowest: (N)</label>
        <div class="col-sm-1">
            <input id="txtLowWeight" asp-for="LowestWeight" class="form-control" readonly="readonly" value="@Model.LowestWeight"
                   style="font-family: Verdana;font-size: small;" />
        </div>
        <label class="control-label col-sm-1" asp-for="HighestWeight">Highest: (N)</label>
        <div class="col-sm-1">
            <input id="txtHighWeight" asp-for="HighestWeight" class="form-control" readonly="readonly" value="@Model.HighestWeight"
                   style="font-family: Verdana;font-size: small;" />
        </div>

        <label class="control-label col-sm-1" style="width:6%" asp-for="WeightUnit">Unit:</label>
        <div class="col-sm-2">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="WeightUnit">
                <option value="1">Newton (N)</option>
                <option value="2">Gram (g)</option>
                <option value="3">Kilogram (kg)</option>
                <option value="4">Pound (lb)</option>
            </select>
        </div>

        <label class="col-sm-2 control-label" style="width:11%" asp-for="WeightNoti">Notification:</label>
        <div class="col-sm-1" style="width:10%">
            <select disabled class="form-control" style="font-family: Verdana;font-size: small;" asp-for="WeightNoti">
                <option value="1">On</option>
                <option value="0">Off</option>
            </select>
        </div>
        <div class="col-sm-1">
            @*<a id="weightEdit">Edit</a>*@
            <button type="button" class="btn-info edit" data-toggle="modal" data-target="#editWeight">Edit</button>
            <input id="weightSave" class="btn btn-primary" type="submit" value="Save">
            <input id="weightReset" class="btn btn-primary" type="reset" value="Reset">
        </div>
    </div>
</form>




<form asp-controller="Preference" asp-action="Setting" method="post" role="form" class="form-horizontal">
    @*Temperature*@
    <div class="modal fade" id="editTemp" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h3 class="text-primary">Temperature</h3>
                </div>
                <div class="modal-body">
                    <div class="row">
                        @*<form asp-controller="Preference" asp-action="Setting_t" method="post" role="form" class="form-horizontal">*@
                        <div class="form-group">
                            <label class="control-label col-sm-4" asp-for="LowestTemp">Lowest: ('C)</label>
                            <div class="col-sm-6">
                                <input id="lowTemp" class="form-control" asp-for="LowestTemp" value="@Model.LowestTemp" placeholder="Please Enter Your Value" />
                                <div class="has-error">
                                    <span id="erlowTemp" asp-validation-for="LowestTemp" class="text-danger" value=""></span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
