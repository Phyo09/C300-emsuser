                    $("#erhigh" + name).html("Please enter number higher than " + $("#low" + name).val() + "!");
                    check = 0;
                }
                else {
                    $("#erhigh"+name).html("");
                    $("#erlow" + name).html("");
                    check = 1;
                }
            }
            else {
                $("#erhigh" + name).html("Please enter number!");
                check = 0;
            }

        }
        function checkValid(name) {
            if (check == 0) {
                $(name).prop('disabled', true);
                //$("#saveHum").attr('disabled', 'disabled');

            }
            else {
                $(name).prop('disabled', false);
                //$("#saveHum").removeAttr('disabled');
