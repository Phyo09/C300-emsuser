                                <select id="drpTemp1" asp-for="TempUnit" class="form-control" style="font-family: Verdana;font-size: small;" value="@Model.TempUnit">
                                    <option value="1">Celcius ('C)</option>
                                    <option value="2">Kelvin (K)</option>
                                    <option value="3">Fahrenheit ('F)</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label" asp-for="TempNoti">Notification:</label>
