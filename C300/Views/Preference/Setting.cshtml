                        </div>

                        <div class="form-group">
                            <div class="col-sm-4"></div>
                            <div class="col-sm-6">
                                <input disabled="disabled" id="saveTemp" type="submit" value="Save" class="btn-info save">
                                <input onclick="reset()" type="reset" value="Reset" class="btn reset">
                            </div>
                        </div>
                        @*</form>*@
