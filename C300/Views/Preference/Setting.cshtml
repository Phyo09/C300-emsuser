                    $("#erlow" + name).html("Please enter number lower than " + $("#high" + name).val() + "!");
                    check = 0;
                }
                else {
                    $("#erlow"+name).html("");
                    $("#erhigh" + name).html("");
                    check = 1;
                }
            }
            else {
                $("#erlowTemp").html("Please enter number!");
